{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": [
    "eslint:recommended",
    "google",
    "plugin:react/recommended"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true,
      "jsx": true,
      "modules": true
    },
    "sourceType": "module"
  },
  "rules": {
    "react/no-unknown-property": [2, {"ignore" : ["class"]}],
    "react/prop-types": 0
  },
  "settings": {
    "react": {
      "pragma": "h"
    }
  }
}
